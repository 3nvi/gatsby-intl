{
  "command": {
    "create": {
      "license": "MIT"
    },
    "bootstrap": {
      "npmClientArgs": ["--no-package-lock"]
    },
    "publish": {
      "allowBranch": "master",
      "npmClientArgs": ["--no-package-lock"],
      "bump": "patch",
      "conventionalCommits": true,
      "message": "chore(release): publish",
      "ignoreChanges": ["CHANGELOG.md", "**/__tests__/**"]
    },
    "version": {
      "conventionalCommits": true
    }
  },
  "packages": ["packages/*"],
  "version": "0.1.24",
  "registry": "https://registry.npmjs.org/"
}
